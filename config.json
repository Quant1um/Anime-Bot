{
  "booru": "safebooru",
  "connection": {
    "__comment": "#{name} means that value will be taken from environment variables.",
    "port": "#PORT",
    "tls": false
  },
  "database": {
    "filename": "database.json",
    "verbose": false,
    "autosaveInterval": 4000
  },
  "vk": {
    "accessToken": "#ACCESS_TOKEN",
    "confirmationCode": "#CONFIRMATION_CODE",
    "secretKey": "#SECRET_KEY"
  }
}
