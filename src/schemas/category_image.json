{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "category_image.json",
  "title": "Config image category",
  "type": "object",
  "properties": {
    "maxWidth": {
      "anyOf": [
        {
          "type": "integer",
          "minimum": 1
        },
        {
          "type": "null"
        }
      ]
    },
    "maxHeight": {
      "anyOf": [
        {
          "type": "integer",
          "minimum": 1
        },
        {
          "type": "null"
        }
      ]
    },
    "quality": {
      "type": "integer",
      "minimum": 1,
      "maximum": 100
    }
  },
  "required": [ "quality" ],
  "additionalProperties": false
}